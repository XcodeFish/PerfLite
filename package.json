{
  "name": "perflite",
  "version": "1.0.0",
  "description": "前端性能监控SDK",
  "type": "module",
  "main": "dist/perflite.min.js",
  "module": "dist/perflite.esm.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "webpack --config webpack.prod.js",
    "dev": "webpack serve --config webpack.dev.js",
    "build:wasm": "cd rust && cargo build --release --target wasm32-unknown-unknown && wasm-bindgen target/wasm32-unknown-unknown/release/perflite_wasm.wasm --out-dir ../src/parser/wasm/generated",
    "setup:wasm": "cargo install wasm-bindgen-cli --version 0.2.88",
    "setup": "chmod +x ./scripts/setup.sh && ./scripts/setup.sh",
    "setup:hooks": "chmod +x ./scripts/init-husky.sh && ./scripts/init-husky.sh",
    "build:all": "pnpm run build:wasm && pnpm run build",
    "test": "jest --coverage",
    "lint": "eslint --config eslint.config.js .",
    "lint:fix": "eslint --config eslint.config.js . --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepare": "husky install",
    "analyze": "webpack --config webpack.analyze.js",
    "prepublishOnly": "pnpm run build:all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/perflite.git"
  },
  "keywords": [
    "performance",
    "monitoring",
    "sdk",
    "error-tracking",
    "wasm"
  ],
  "author": "Your Name",
  "license": "MIT",
  "dependencies": {
    "echarts": "^5.4.3",
    "lru-cache": "^7.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.22.10",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/preset-env": "^7.22.10",
    "@commitlint/cli": "^17.6.7",
    "@commitlint/config-conventional": "^17.6.7",
    "@types/jest": "^29.5.3",
    "@types/md5": "^2.3.5",
    "@typescript-eslint/eslint-plugin": "^6.3.0",
    "@typescript-eslint/parser": "^6.3.0",
    "babel-loader": "^9.1.3",
    "compression-webpack-plugin": "^10.0.0",
    "css-minimizer-webpack-plugin": "^5.0.1",
    "eslint": "^9.27.0",
    "eslint-import-resolver-typescript": "^3.6.0",
    "eslint-plugin-import": "^2.28.0",
    "eslint-plugin-jsdoc": "^46.4.6",
    "husky": "^8.0.3",
    "jest": "^29.6.2",
    "lint-staged": "^13.2.3",
    "md5": "^2.3.0",
    "prettier": "^3.5.3",
    "rollup": "^3.28.0",
    "rollup-plugin-terser": "^7.0.2",
    "string-replace-loader": "^3.1.0",
    "terser-webpack-plugin": "^5.3.9",
    "ts-jest": "^29.1.1",
    "ts-loader": "^9.4.4",
    "typescript": "^5.1.6",
    "wasm-loader": "^1.3.0",
    "webpack": "^5.88.2",
    "webpack-bundle-analyzer": "^4.9.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^4.15.1",
    "webpack-merge": "^5.9.0"
  },
  "sideEffects": false,
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ]
}
